{
    "main": "node_modules/expo/AppEntry.js",
    "engines": {
        "node": ">=12.13.0"
    },
    "engineStrict": true,
    "scripts": {
        "start": "expo start",
        "android": "expo start --android",
        "ios": "expo start --ios",
        "web": "expo start --web",
        "eject": "expo eject",
        "start:localhost": "expo start --localhost",
        "lint:ts": "tslint -c tslint.json --project .",
        "lint:ts:fix": "yarn run lint:ts -- --fix",
        "lint:staged": "lint-staged",
        "test:staged": "npm test",
        "prettify": "prettier --write",
        "test": "NODE_ENV=test jest",
        "e2e": "detox test",
        "ci": "yarn test --no-cache --collectCoverage --collectCoverageFrom=src/**/*.{ts,tsx,js} --maxWorkers=2 --silent --ci"
    },
    "dependencies": {
        "@expo/vector-icons": "^10.0.1",
        "@react-native-community/masked-view": "0.1.6",
        "@react-navigation/native": "^5.4.0",
        "@types/jwt-decode": "^2.2.1",
        "@types/react-native-dotenv": "^0.2.0",
        "axios": "^0.19.2",
        "enzyme-to-json": "^3.4.4",
        "expo": "~37.0.3",
        "expo-auth-session": "https://github.com/TransformCore/expo-auth-session",
        "expo-font": "^8.1.1",
        "expo-local-authentication": "~9.0.0",
        "expo-secure-store": "~8.1.0",
        "expo-web-browser": "~8.2.1",
        "jwt-decode": "^2.2.0",
        "mobx": "^5.15.4",
        "mobx-react": "^6.2.2",
        "mobx-react-lite": "^2.0.7",
        "mockdate": "^3.0.2",
        "querystring": "^0.2.0",
        "react": "~16.9.0",
        "react-dom": "~16.9.0",
        "react-is": "^16.13.1",
        "react-native": "https://github.com/expo/react-native/archive/sdk-37.0.1.tar.gz",
        "react-native-credit-card-input": "^0.4.1",
        "react-native-dropdownalert": "^4.2.1",
        "react-native-easy-grid": "^0.2.2",
        "react-native-gesture-handler": "~1.6.0",
        "react-native-reanimated": "~1.7.0",
        "react-native-safe-area-context": "0.7.3",
        "react-native-screens": "~2.2.0",
        "react-native-status-bar-height": "^2.5.0",
        "react-native-tableview-simple": "^3.2.0",
        "react-native-web": "~0.11.7",
        "react-native-webview": "8.1.1",
        "react-navigation": "^4.3.9",
        "react-navigation-stack": "^2.5.1",
        "styled-components": "^5.1.0",
        "types": "^0.1.1"
    },
    "devDependencies": {
        "@babel/core": "^7.8.6",
        "@storybook/addon-actions": "^5.3.18",
        "@storybook/addon-knobs": "^5.3.18",
        "@storybook/addon-notes": "^5.3.18",
        "@storybook/addon-ondevice-notes": "^5.3.18",
        "@storybook/addons": "^5.3.18",
        "@storybook/react-native": "^5.3.18",
        "@types/enzyme": "^3.10.5",
        "@types/enzyme-adapter-react-16": "^1.0.6",
        "@types/jest": "^25.2.1",
        "@types/jsdom": "^16.2.1",
        "@types/react": "~16.9.23",
        "@types/react-native": "~0.61.17",
        "@types/react-test-renderer": "^16.9.2",
        "@types/storybook__react-native": "^5.2.1",
        "@types/styled-components": "^5.1.0",
        "babel-preset-expo": "~8.1.0",
        "detox": "^16.5.0",
        "detox-expo-helpers": "^0.6.0",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.2",
        "expo-cli": "^3.21.3",
        "expo-detox-hook": "^1.0.10",
        "husky": "^4.2.5",
        "jest": "^25.0.0",
        "jest-expo": "^37.0.0",
        "jsdom": "^16.2.2",
        "lint-staged": "^10.2.2",
        "prettier": "^2.0.5",
        "react-test-renderer": "^16.13.1",
        "sonarqube-scanner": "^2.6.0",
        "ts-jest": "^25.5.1",
        "tslint": "^6.1.2",
        "tslint-config-prettier": "1.18.0",
        "tslint-eslint-rules": "^5.4.0",
        "tslint-react": "^5.0.0",
        "typescript": "~3.8.3"
    },
    "lint-staged": {
        "src/**/*.{ts,tsx}": [
            "yarn run prettify",
            "yarn run lint:ts:fix",
            "yarn run test:staged",
            "git add --force"
        ],
        "*.json": [
            "yarn run prettify",
            "git add --force"
        ]
    },
    "private": true,
    "detox": {
        "test-runner": "jest",
        "configurations": {
            "ios.sim": {
                "binaryPath": "bin/Exponent.app",
                "type": "ios.simulator",
                "name": "iPhone 11 Pro Max"
            }
        }
    }
}
